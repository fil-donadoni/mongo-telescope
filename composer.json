{
  "name": "fil-donadoni/mongo-telescope",
  "description": "Laravel Package to use Telescope with MongoDB and without MySql",
  "keywords": [
    "laravel",
    "eloquent",
    "mongodb",
    "mongo",
    "database",
    "model",
    "telescope"
  ],
  "authors": [
    { "name": "Filippo Donadoni", "homepage": "https://filippodonadoni.it", "role": "Creator" }
  ],
  "license": "MIT",
  "type": "library",
  "require": {
    "php": ">=8.0",
    "illuminate/support": "^8.0|^9.0|^10.0",
    "mongodb/laravel-mongodb": "^5.0"
  },
  "require-dev": {
    "laravel/telescope": "^5.0"
  },
  "autoload": {
    "psr-4": {
      "FilDonadoni\\MongoTelescope\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "FilDonadoni\\MongoTelescope\\Providers\\TelescopeServiceProvider"
      ]
    }
  }
}